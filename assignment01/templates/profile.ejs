<%- include("_header") %>

<% if (typeof user === "undefined") {
    user = {};
} %>
<div class="container">
    <div class="row justify-content-center">
        <div class="col-12 col-md-8 col-lg-6">
            <h1>Profile</h1>
            <p>ID: <%= user.id %></p>
            <p>Email: <%= user.email %></p>
            <p>Username: <%= user.username %></p>
            <a href="/logout" class="btn btn-primary">Logout</a>
        </div>
    </div>
</div>

<%- include("_footer") %>
